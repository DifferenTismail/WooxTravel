@model List<Project2WooxTravel.Models.Entities.Destination>
<div class="cities-town">
    <div class="container">
        <div class="row">
            <div class="slider-content">
                <div class="row">
                    <div class="col-lg-12">
                        <h2>Diğer <em>Turlarımız</em></h2>
                    </div>
                    <div class="col-lg-12">
                        <div class="owl-cites-town owl-carousel">
                            @foreach (var item in Model)
                            {
                                <div class="item">
                                    <div class="thumb">
                                        <img style="height:182px; width:341px;" src="@item.ImageUrl" alt="">
                                        <h4>@item.Country</h4>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>